<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>{{cookiecutter.__example_messages_package_name}}</name>
    <version>{{cookiecutter.example_package_version}}</version>
    <description>Defines messages used by the '{{cookiecutter.__project_name_slug}} project.</description>
    <maintainer email="{{cookiecutter.email}}">{{cookiecutter.github_username_or_org}}
    </maintainer>
    <license>{{cookiecutter.license}}</license>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <build_depend>rosidl_default_generators</build_depend>

    <exec_depend>rosidl_default_runtime</exec_depend>

    <member_of_group>rosidl_interface_packages</member_of_group>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
